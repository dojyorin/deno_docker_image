{
    "name": "test",
    "on": {
        "push": {
            "branches-ignore": [
                "master"
            ],
            "paths-ignore": [
                ".git*",
                "**.md"
            ]
        },
        "pull_request": {
            "branches": [
                "master"
            ],
            "paths-ignore": [
                ".git*",
                "**.md"
            ]
        }
    },
    "jobs": {
        "test": {
            "name": "test: ubuntu-latest",
            "runs-on": "ubuntu-latest",
            "steps": [{
                "name": "clone repository",
                "uses": "actions/checkout@v3"
            }]
        }
    }
}